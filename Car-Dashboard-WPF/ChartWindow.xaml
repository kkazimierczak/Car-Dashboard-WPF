<Window x:Class="Car_Dashboard_WPF.ChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        xmlns:local="clr-namespace:Car_Dashboard_WPF"
        mc:Ignorable="d"
        Title="ChartWindow" Height="720" Width="400" Closed="Window_Closed">
    <Grid>
        <d3:ChartPlotter Name="SpeedPlot" Margin="10,31,32,458" />
        <d3:ChartPlotter x:Name="RPMPlot" Margin="10,267,32,222" />
        <d3:ChartPlotter x:Name="FuelUsagePlot" Margin="10,489,32,0" />
        <Label x:Name="label" Content="Speed" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,0,0,0"/>
        <Label x:Name="label_Copy" Content="RPM" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,236,0,0"/>
        <Label x:Name="label_Copy1" Content="Fuel usage" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,458,0,0"/>
    </Grid>
</Window>
